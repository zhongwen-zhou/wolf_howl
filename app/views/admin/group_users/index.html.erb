<h1>Listing group_users</h1>

<table>
  <tr>
    <th>Group</th>
    <th>User</th>
    <th>Is admin</th>
    <th>Invitees</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @group_users.each do |group_user| %>
  <tr>
    <td><%= group_user.group_id %></td>
    <td><%= group_user.user_id %></td>
    <td><%= group_user.is_admin %></td>
    <td><%= group_user.invitees_id %></td>
    <td><%= link_to 'Show', group_user %></td>
    <td><%= link_to 'Edit', edit_group_user_path(group_user) %></td>
    <td><%= link_to 'Destroy', group_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

